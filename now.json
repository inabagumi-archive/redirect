{
  "alias": [
    "www.animare.cafe",
    "www.haneru.dev",
    "www.neru.camera",
    "heartbeat.haneru.dev"
  ],
  "builds": [
    {
      "src": "static/**/*.js",
      "use": "@now/static"
    },
    {
      "src": "**/*.go",
      "use": "@now/go"
    }
  ],
  "github": {
    "silent": true
  },
  "routes": [
    {
      "dest": "static/service-worker.js",
      "src": "/(?:sw|service-worker)\\.js"
    },
    {
      "dest": "index.go",
      "src": "/.*"
    }
  ],
  "version": 2
}
